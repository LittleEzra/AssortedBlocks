{
  "multipart": [
    {
      "apply": {
        "model": "artistry:block/mangrove_table_base"
      }
    },
    {
      "apply": {
        "model": "artistry:block/mangrove_table_northwest_leg"
      },
      "when": {
        "north_west": "true"
      }
    },
    {
      "apply": {
        "model": "artistry:block/mangrove_table_northeast_leg"
      },
      "when": {
        "north_east": "true"
      }
    },
    {
      "apply": {
        "model": "artistry:block/mangrove_table_southeast_leg"
      },
      "when": {
        "south_east": "true"
      }
    },
    {
      "apply": {
        "model": "artistry:block/mangrove_table_southwest_leg"
      },
      "when": {
        "south_west": "true"
      }
    }
  ]
}